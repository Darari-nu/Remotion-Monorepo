[
  {
    "id": 0,
    "start": 0.0,
    "end": 19.36,
    "text": "♪ 午前二時 突然の悪夢"
  },
  {
    "id": 1,
    "start": 19.36,
    "end": 23.32,
    "text": "午前二時 突然の悪夢"
  },
  {
    "id": 2,
    "start": 23.32,
    "end": 28.04,
    "text": "窓の外は静かな雨音"
  },
  {
    "id": 3,
    "start": 28.04,
    "end": 32.14,
    "text": "あの子の好きな優しい音"
  },
  {
    "id": 4,
    "start": 32.14,
    "end": 36.16,
    "text": "耳を澄ませばさっきの恐怖は"
  },
  {
    "id": 5,
    "start": 36.16,
    "end": 41.48,
    "text": "まるで爆が食べちゃったみたい"
  },
  {
    "id": 6,
    "start": 41.48,
    "end": 45.6,
    "text": "昨日見た寂しそうな横顔"
  },
  {
    "id": 7,
    "start": 45.6,
    "end": 50.0,
    "text": "あれは僕の気のせい"
  },
  {
    "id": 8,
    "start": 50.0,
    "end": 54.68,
    "text": "あの子には叩きつける嵐じゃなく"
  },
  {
    "id": 9,
    "start": 54.68,
    "end": 58.02,
    "text": "優しい雨音"
  },
  {
    "id": 10,
    "start": 58.04,
    "end": 64.6,
    "text": "その音だけ届けばいいのに"
  },
  {
    "id": 11,
    "start": 64.6,
    "end": 67.32,
    "text": "ねえ夢食いバーク"
  },
  {
    "id": 12,
    "start": 67.32,
    "end": 69.56,
    "text": "もしもいるのなら"
  },
  {
    "id": 13,
    "start": 69.56,
    "end": 73.6,
    "text": "どうか今夜あの子の夢を守って"
  },
  {
    "id": 14,
    "start": 73.6,
    "end": 75.56,
    "text": "ささくれみたいな"
  },
  {
    "id": 15,
    "start": 75.56,
    "end": 78.56,
    "text": "後悔の戸を閉める言葉"
  },
  {
    "id": 16,
    "start": 78.56,
    "end": 81.14,
    "text": "全部悪い夢にして"
  },
  {
    "id": 17,
    "start": 81.14,
    "end": 86.14,
    "text": "立て尽くして"
  },
  {
    "id": 18,
    "start": 88.04,
    "end": 90.04,
    "text": "逃げ出せることはない"
  },
  {
